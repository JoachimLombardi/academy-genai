        {
          "name": "text",
          "description": "Full text of the knowledge base article",
          "dataType": ["text"],
          "moduleConfig": {
            "text2vec-transformers": {
              "skip": false,
              "vectorizePropertyName": true
            }
          },
          "indexFilterable": true,
          "indexSearchable": true
        }
      ]
    }
  ]
}
